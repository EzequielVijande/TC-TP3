\end_inset


\end_layout

\begin_layout Plain Layout

\end_layout

\end_inset


\end_layout

\begin_layout Standard
Luego:
\end_layout

\begin_layout Standard
\begin_inset Formula $(V^{+}-V_{th})sC_{G}=\frac{(V_{th}-V^{-})}{R_{L}}$
\end_inset


\end_layout

\begin_layout Standard
considerando 
\begin_inset Formula $V^{+}=V^{-}\Rightarrow(V^{-}-V_{th})sC_{G}=\frac{(V_{th}-V^{-})}{R_{L}}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\Rightarrow V_{th}(sC_{G}+\frac{1}{R_{L}})=V^{-}(sC_{G}+\frac{1}{R_{L}})\Rightarrow V_{th}=V^{-}=V^{+}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\Rightarrow I=0\Rightarrow\left[V_{th}=V_{in}\right]$
\end_inset


\end_layout

